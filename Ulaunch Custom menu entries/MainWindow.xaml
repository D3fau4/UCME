        <TextBox x:Name="NROargvBox" HorizontalAlignment="Left" Height="23" Margin="54.696,266.227,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Text="sdmc:/"/>
        <TextBlock x:Name="textBlock5" HorizontalAlignment="Left" Margin="54.696,297.477,0,0" TextWrapping="Wrap" Text="Type:" VerticalAlignment="Top"/>
        <TextBox x:Name="TypeBox" HorizontalAlignment="Left" Height="23" Margin="54.696,319.427,0,0" TextWrapping="Wrap" Text="2" VerticalAlignment="Top" Width="120" IsEnabled="False"/>

