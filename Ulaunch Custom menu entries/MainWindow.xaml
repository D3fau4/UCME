        mc:Ignorable="d"
        Title="UCME by D3fau4" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Namebox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Margin="54.696,70.387,0,0"/>
